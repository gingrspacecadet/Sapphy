{
    "name": "sapphy",
    "compatibility_date": "2024-04-25",
    "pages_build_output_dir": ".",
    "functions": {
      "directory": "functions"
    },
    "d1_databases": [
      {
        "binding": "DB",
        "database_name": "your-database-name",
        "database_id": "your-database-id"
      }
    ],
    "triggers": {
      "crons": ["0 0 * * *"]
    },
    "build": {
      "upload": {
        "dir": "scheduled",
        "main": "./worker.js",
        "format": "modules"
      }
    },
    "vars": {
      "MAILGUN_API_KEY": "your-mailgun-api-key",
      "MAILGUN_DOMAIN": "your-mailgun-domain.com"
    }
}  